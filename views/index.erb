<center>
  <h1 class="display-4 py-3 mb-3 border-bottom">
    <%= title %>
  </h1>

  <h4>OSC SI 2023</h4>
  <h3>Create Some Projects!</h3>
  <br>
  <h2 class="my-4">Current Projects</h2>
  <div class="row-my-5">
    <ol class="list-group list-group-horizontal flex-wrap col-md-12">
    <% @project_dirs.each do |dir| %>
    <li class="list-group-item btn btn-outline-dark m-3 border">
      <div><a href="<%= url("/projects/#{dir}") %>" class="text-center">
        <a href="<%= url("/delete/#{dir}") %>" class="nav-link active" aria-current="page" <svg class="bi me-2" width="3" height="16"><use xlink.href="#delete">Delete</use></svg>></a>
        <i class="fa fa-video fa-5x"></i>
        <p><%= dir.gsub('_',' ').capitalize %></p>
      </a>
    </div>
    </li>
    <% end %>
  </ol>
  </div>

  

  </center>
